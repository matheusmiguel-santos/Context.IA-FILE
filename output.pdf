  if (code === plus || code === minus) {
    nextCode = value.charCodeAt(1)

    if (nextCode >= 48 && nextCode <= 57) {
      return true
    }

    var nextNextCode = value.charCodeAt(2)

    if (nextCode === dot && nextNextCode >= 48 && nextNextCode <= 57) {
      return true
    }

    return false
  }

  if (code === dot) {
    nextCode = value.charCodeAt(1)

    if (nextCode >= 48 && nextCode <= 57) {
      return true
    }

    return false
  }

  if (code >= 48 && code <= 57) {
    return true
  }

  return false
